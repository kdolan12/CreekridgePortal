<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>CreekRidgeCapital</portlet-name>
		<display-name>CreekRidgeCapital Home</display-name>
		<portlet-class>com.tamarack.liferay.payment.HomePageNavigation</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/homepageNavigate/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>CreekRidgeCapital</title>
			<short-title>CreekRidgeCapital</short-title>
			<keywords>CreekRidgeCapital</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>paymentcalculatortable</portlet-name>
		<display-name>Payment Calculator Table</display-name>
		<portlet-class>com.tamarack.liferay.payment.PaymentCalculatorTable
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/paymentcalculatortable/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Payment Calculator Table</title>
			<short-title>Payment Calculator Table</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
	
	
	<portlet>
		<portlet-name>viewCreditApplicationPortlet</portlet-name>
		<display-name>View Credit Application</display-name>
		<portlet-class>com.tamarack.liferay.payment.ViewApplication
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/viewCreditApplication/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>View Credit Application</title>
			<short-title>View Credit Application</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
	<portlet>
		<portlet-name>insertPaymentCalculatorLookValuesPortlet</portlet-name>
		<display-name>Payment Calculator Look Values </display-name>
		<portlet-class>com.tamarack.liferay.payment.InsertPaymentCalculatorLookValuesPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/lookValuesPortlet/view.jsp</value>
		</init-param>
		<init-param>
			<name>add-process-action-success-action</name>
			<value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Payment Calculator Look Values</title>
			<short-title>Payment Calculator Look Values</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
</portlet-app>